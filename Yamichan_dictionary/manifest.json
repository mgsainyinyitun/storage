{
   "background": {
      "page": "html/background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": "img/yomichan_disable.png",
      "default_title": "選択したテキストにふりがなを付ける"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "css/style.css" ],
      "js": [ "js/yomichanContent.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "description": "選択したテキストにふりがなを付ける",
   "homepage_url": "https://github.com/guoweihua1982/yomichan",
   "icons": {
      "128": "img/icon.png",
      "16": "img/icon.png",
      "48": "img/icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "yomichan",
   "omnibox": {
      "keyword": "yomi"
   },
   "permissions": [ "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.2",
   "web_accessible_resources": [ "img/shrinkButton.png", "img/icon.png" ]
}
